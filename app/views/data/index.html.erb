<p id="notice"><%= notice %></p>

<h1>Listing Data</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Email type</th>
      <th>Event</th>
      <th>Timestamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |datum| %>
      <tr>
        <td><%= datum.address %></td>
        <td><%= datum.email_type %></td>
        <td><%= datum.event %></td>
        <td><%= datum.timestamp %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
