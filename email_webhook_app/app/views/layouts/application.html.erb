<!DOCTYPE html>
<html>
  <head>
    <title>EmailWebhookApp</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header class='header'>
      <%= image_tag "logo.svg" %>
    </header>

  <%= yield %>

    <hr noshade class='divider'> 
    <footer class='footer'>
      <span id='logoFooter'><%= image_tag "footer.svg" %></span>
    </footer>

  </body>
</html>
